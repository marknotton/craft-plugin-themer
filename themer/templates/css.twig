{% set css %}

#poweredby { display:none; }

#global-sidebar { background-color:{{ settings.sidebar }} }

body, html {
  background-color:{{ settings.background }}
}

/* LOGIN */
body.login {
  /*background:linear-gradient(col(white, 0.80), col(white, 0.80)), image-url($craft-backround-image);*/
  background-color: {{settings.background}};
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  /*#login-form { background:image-url($craft-logo) no-repeat center 0; background-size:100% auto; padding-top:$craft-logo-height;
    #ssl-icon { top:7px; }
    label, #forgot-password { font-size:12px; }
  }*/ }
  body.login #forgot-password {
    color: {{ settings.text }};
    transition:  color 0.2s ease-in-out ; }
    body.login #forgot-password:hover {
      color: {{settings.links}}; }
  body.login .btn.disabled {
    display: block; }


.field[id*="facebook"]:before {
  position: relative;
  display: block;
  background: url("data:image/svg+xml;base64, ") center center / 100% 100% no-repeat;
  background-color: #3b5998; }
  .field[id*="facebook"]:before:before {
    content: "";
    display: block;
    width: 100%;
    padding-top: 100%; }
  .field[id*="facebook"]:before > span {
    position: absolute;
    text-indent: 110%;
    overflow: hidden;
    white-space: nowrap;
    -moz-user-select: -moz-none;
    -ms-user-select: none;
    -webkit-user-select: none;
    user-select: none;
    color: transparent;
    font-size: 0;
    opacity: 0; }
